// 9. Calcular cu√°ntos pesos tiene un banco en monedas si dispone de N1 monedas de 1
// peso, N2 de 50 centavos, N3 de 25 centavos, N4 de 10 centavos y N5 de 5 centavos.

Console.WriteLine("ingrese la cantidad de monedas de un peso: ");
int m1 = int.Parse(Console.ReadLine());

Console.WriteLine("ingrese la cantidad de monedas de 50 centavos: ");
double m2 =  double.Parse(Console.ReadLine()) * 0.5;

Console.WriteLine("ingrese la cantidad de monedas de 25 centavos: ");
double m3 = double.Parse(Console.ReadLine()) * 0.25;

Console.WriteLine("ingrese la cantidad de monedas de 10 centavos: ");
double m4 = double.Parse(Console.ReadLine()) * 0.10;

Console.WriteLine("ingrese la cantidad de monedas de 5 centavos: ");
double m5 = double.Parse(Console.ReadLine()) * 0.05;

double suma = m1 + m2 + m3 + m4 + m5;
Console.WriteLine("el valor total en pesos es de: " + suma);
